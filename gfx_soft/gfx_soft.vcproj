				Name="VCLinkerTool"
				AdditionalOptions="/MACHINE:I386"
				AdditionalDependencies="dxguid.lib ddraw.lib odbc32.lib odbccp32.lib"
				OutputFile=".\Release/gfx_soft.dll"
				LinkIncremental="1"
				SuppressStartupBanner="TRUE"
				AdditionalLibraryDirectories="..\common\,$(PYTHON)\lib,..\3rdparty"
				ModuleDefinitionFile="..\common\gfx_driver.def"
				ProgramDatabaseFile=".\Release/gfx_soft.pdb"
