    <style>wxDEFAULT_DIALOG_STYLE|wxDIALOG_MODELESS</style>
  </object>
  <object class="wxDialog" name="dialog_zone">
    <object class="wxPanel" name="panel_main">
      <object class="wxBoxSizer">
        <orient>wxHORIZONTAL</orient>
        <object class="sizeritem">
          <object class="wxListBox" name="list_zones"/>
        </object>
        <object class="sizeritem">
          <object class="wxBoxSizer">
            <orient>wxVERTICAL</orient>
            <object class="sizeritem">
              <object class="wxFlexGridSizer">
                <object class="sizeritem">
                  <object class="wxStaticText">
                    <label>Name</label>
                  </object>
                </object>
                <object class="sizeritem">
                  <object class="wxTextCtrl" name="edit_name"/>
                </object>
                <object class="sizeritem">
                  <object class="wxStaticText">
                    <label>Description</label>
                  </object>
                </object>
                <object class="sizeritem">
                  <object class="wxTextCtrl" name="edit_description"/>
                </object>
                <object class="sizeritem">
                  <object class="wxStaticText">
                    <label>Script</label>
                  </object>
                </object>
                <object class="sizeritem">
                  <object class="wxTextCtrl" name="edit_script"/>
                </object>
                <object class="sizeritem">
                  <object class="wxStaticText">
                    <label>Chance (%)</label>
                  </object>
                </object>
                <object class="sizeritem">
                  <object class="wxTextCtrl" name="edit_chance"/>
                </object>
                <cols>2</cols>
                <object class="sizeritem">
                  <object class="wxStaticText">
                    <label>Delay</label>
                  </object>
                </object>
                <object class="sizeritem">
                  <object class="wxTextCtrl" name="edit_delay"/>
                </object>
              </object>
            </object>
            <object class="sizeritem">
              <object class="wxCheckBox" name="check_adjactivate">
                <label>&amp;Adjacent Activation</label>
              </object>
            </object>
            <object class="spacer">
              <size>0,10</size>
            </object>
            <object class="sizeritem">
              <object class="wxBoxSizer">
                <object class="sizeritem">
                  <object class="wxButton" name="wxID_OK">
                    <label>Ok</label>
                    <default>1</default>
                  </object>
                </object>
                <object class="spacer">
                  <size>10</size>
                </object>
                <object class="sizeritem">
                  <object class="wxButton" name="button_newzone">
                    <label>New &amp;Zone</label>
                  </object>
                </object>
                <object class="sizeritem">
                  <object class="wxButton" name="button_deletezone">
                    <label>&amp;Delete</label>
                  </object>
                </object>
              </object>
            </object>
          </object>
        </object>
      </object>
    </object>
    <size>400,300</size>
    <style>wxDEFAULT_DIALOG_STYLE|wxDIALOG_MODELESS</style>
  </object>
  <object class="wxDialog" name="dialog_newmap">
    <size>320,240</size>
    <object class="wxPanel" name="panel_main">
      <object class="wxBoxSizer">
        <orient>wxVERTICAL</orient>
        <object class="sizeritem">
          <object class="wxGridSizer">
            <cols>2</cols>
            <object class="sizeritem">
              <object class="wxStaticText">
                <label>&amp;Width</label>
              </object>
            </object>
            <object class="sizeritem">
              <object class="wxTextCtrl" name="edit_width"/>
              <flag>wxEXPAND</flag>
            </object>
            <object class="sizeritem">
              <object class="wxStaticText">
                <label>&amp;Height</label>
              </object>
            </object>
            <object class="sizeritem">
              <object class="wxTextCtrl" name="edit_height"/>
              <flag>wxEXPAND</flag>
            </object>
            <object class="sizeritem">
              <object class="wxStaticText">
                <label>&amp;Tileset Filename</label>
              </object>
            </object>
            <object class="sizeritem">
              <object class="wxBoxSizer">
                <object class="sizeritem">
                  <object class="wxTextCtrl" name="edit_tileset"/>
                </object>
                <object class="sizeritem">
                  <object class="wxButton" name="button_browse">
                    <label>...</label>
                  </object>
                  <minsize>20,-1</minsize>
                </object>
              </object>
            </object>
          </object>
        </object>
        <object class="spacer">
          <size>10,10</size>
        </object>
        <object class="sizeritem">
          <object class="wxBoxSizer">
            <object class="spacer">
              <size>10,10</size>
            </object>
            <object class="sizeritem">
              <object class="wxButton" name="wxID_OK">
                <label>Ok</label>
                <style/>
                <default>1</default>
              </object>
            </object>
            <object class="spacer">
              <size>10,10</size>
            </object>
            <object class="sizeritem">
              <object class="wxButton" name="wxID_CANCEL">
                <label>Cancel</label>
              </object>
            </object>
          </object>
        </object>
      </object>
    </object>
    <title>New Map</title>
    <style>wxDEFAULT_DIALOG_STYLE|wxDIALOG_MODAL</style>
  </object>
  <object class="wxDialog" name="dialog_importchr">
    <title>Import CHR</title>
    <style>wxDEFAULT_DIALOG_STYLE|wxDIALOG_MODAL</style>
    <object class="wxPanel" name="panel_main">
      <object class="wxBoxSizer">
        <orient>wxVERTICAL</orient>
        <object class="sizeritem">
          <object class="wxFlexGridSizer">
            <cols>2</cols>
            <object class="sizeritem">
              <object class="wxStaticText">
                <label>Frame &amp;Width</label>
              </object>
            </object>
            <object class="sizeritem">
              <object class="wxTextCtrl" name="edit_framewidth"/>
              <flag>wxEXPAND</flag>
            </object>
            <object class="sizeritem">
              <object class="wxStaticText">
                <label>Frame &amp;Height</label>
              </object>
            </object>
            <object class="sizeritem">
              <object class="wxTextCtrl" name="edit_frameheight"/>
              <flag>wxEXPAND</flag>
            </object>
            <object class="sizeritem">
              <object class="wxStaticText">
                <label>&amp;Number of Frames</label>
              </object>
            </object>
            <object class="sizeritem">
              <object class="wxTextCtrl" name="edit_numframes"/>
              <flag>wxEXPAND</flag>
            </object>
            <object class="sizeritem">
              <object class="wxStaticText">
                <label>Frames per &amp;Row</label>
              </object>
            </object>
            <object class="sizeritem">
              <object class="wxTextCtrl" name="edit_framesperrow"/>
              <flag>wxEXPAND</flag>
            </object>
            <object class="sizeritem">
              <object class="wxStaticText">
                <label>&amp;Filename</label>
              </object>
            </object>
            <object class="sizeritem">
              <object class="wxBoxSizer">
                <object class="sizeritem">
                  <object class="wxTextCtrl" name="edit_filename"/>
                </object>
                <object class="sizeritem">
                  <object class="wxButton" name="button_browse">
                    <label>...</label>
                    <size>20,20</size>
                  </object>
                </object>
              </object>
            </object>
          </object>
        </object>
        <object class="sizeritem">
          <object class="wxCheckBox" name="check_padding">
            <label>&amp;1 Pixel padding between frames</label>
          </object>
        </object>
        <object class="sizeritem">
          <object class="wxCheckBox" name="check_append">
            <label>&amp;Append frames</label>
            <checked>1</checked>
          </object>
        </object>
        <object class="spacer">
          <size>10,10</size>
        </object>
        <object class="sizeritem">
          <object class="wxBoxSizer">
            <orient>wxHORIZONTAL</orient>
            <object class="sizeritem">
              <object class="wxButton" name="wxID_OK">
                <label>Ok</label>
                <default>1</default>
              </object>
            </object>
            <object class="spacer">
              <size>10,10</size>
            </object>
            <object class="sizeritem">
              <object class="wxButton" name="wxID_CANCEL">
                <label>Cancel</label>
              </object>
            </object>
          </object>
        </object>
      </object>
    </object>
    <size>400,300</size>
  </object>
